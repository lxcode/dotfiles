Xft*dpi: 96
Xft*autohint: 0
Xft*lcdfilter: lcddefault
Xft*hinting: 1
Xft*hintstyle: hintfull
Xft*antialias: 1
Xft*rgba: rgba

XTerm*reverseVideo:   	    on
XTerm*visualBell:   	    off
XTerm*scrollLines:   	    10000
XTerm*faceName: Inconsolata
XTerm*faceSize: 13
XTerm*termName:         xterm-256color

URxvt*reverseVideo:   	    on
URxvt*iso14755: False
URxvt*visualBell:   	    off
URxvt*scrollLines:   	    10000
URxvt*scrollBar:   	    0
URxvt*url-launcher:	/usr/bin/open
URxvt*perl-ext: selection,matcher
URxvt.colorUL: #4682B4
URxvt*matcher.button:	3
URxvt*underlineURLs: true
URxvt*transparent:	off
URxvt*shading:	10
URxvt.font:             xft:Inconsolata-13:Medium:antialias=true:hinting=true xft:Meiryo Normal-13
URxvt.boldfont:         xft:Inconsolata-13:Bold:antialias=true:hinting=true xft:Meiryo Bold-13
URxvt.letterSpace:	 -1
URxvt.imLocale: ja_JP.UTF-8
URxvt.preeditType: OnTheSpot,None
URxvt.urgentOnBell:         true
URxvt.cursorBlink:         false
URxvt.scrollTtyOutput:        false
URxvt.termName: xterm-256color
URxvt.fading: 25
URxvt.keysym.S-Insert: builtin:

