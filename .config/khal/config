[calendars]

[[home]]
    path = ~/.calendars/icloud/home/
    color = dark cyan

[[facebook]]
    path = ~/.calendars/facebook/
    color = dark blue

[[holidays]]
    path = ~/.calendars/holidays/
    color = dark magenta

[[duedates]]
    path = ~/.calendars/duedates/
    color = light red

[[jpholidays]]
    path = ~/.calendars/jpholidays/
    color = dark red

[locale]
    local_timezone= US/Pacific
    default_timezone= US/Pacific
    timeformat= %H:%M
    dateformat= %m.%d.
    longdateformat= %Y-%m-%d
    datetimeformat= %m.%d. %H:%M
    longdatetimeformat= %m.%d.%Y %H:%M
    firstweekday= 6

[view]
    event_view_always_visible = true
    theme=dark
    frame=color
    agenda_event_format = {calendar-color}{cancelled}{start-end-time-style} {title}{repeat-symbol}{reset}

[default]
default_calendar = home
highlight_event_days = true
